<!-- XSD for the PLEXOR devices; Used in CONNEXION; INSPECTOR. The list of devices are presentated in INSPECTOR, the inspector can selected on of the PLEXOR-testequipment to perform the inspection-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PLEXORS">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PLEXOR" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string"/>
							<xs:element name="SerialNumber" type="xs:string"/>
							<xs:element name="BTAddress" type="xs:string"/>
							<xs:element name="PN" type="xs:string"/>
							<xs:element name="CalibrationDate" type="xs:date"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>