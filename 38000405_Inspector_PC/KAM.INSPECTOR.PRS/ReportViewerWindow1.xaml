<Window x:Class="KAM.INSPECTOR.PRS.ReportViewerWindow1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:KAM.INSPECTOR.PRS"
    xmlns:tr="http://schemas.telerik.com/wpf"
    xmlns:telerikReporting="clr-namespace:Telerik.Reporting;assembly=Telerik.Reporting"
    Title="Report Viewer Window">

  <Grid x:Name="LayoutRoot">
    <Grid Margin="20" >
      <Grid.RowDefinitions>
        <RowDefinition Height="*" />
      </Grid.RowDefinitions>

      <tr:ReportViewer Grid.Row="0" x:Name="ReportViewer1" HorizontalAlignment="Stretch">
        <tr:ReportViewer.ReportSource>
          <telerikReporting:UriReportSource Uri="Reports/teset.trdx"/>
        </tr:ReportViewer.ReportSource>
      </tr:ReportViewer>
     
    </Grid>
  </Grid>
</Window>
